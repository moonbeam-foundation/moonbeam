{
  "settings": {
    "timeout": 1000,
    "provider": "native"
  },
  "relaychain": {
    "chain": "rococo-local",
    "default_command": "tmp/polkadot",
    "default_args": [
      "--no-hardware-benchmarks",
      "-lparachain=debug",
      "--database=paritydb"
    ],
    "genesis": {
      "runtimeGenesis": {
        "patch": {
          "configuration": {
            "config": {
              "scheduler_params": {
                "scheduling_lookahead": 2
              },
              "async_backing_params": {
                "max_candidate_depth": 3,
                "allowed_ancestry_len": 2
              }
            }
          }
        }
      }
    },
    "nodes": [
      {
        "name": "alice",
        "validator": true
      },
      {
        "name": "bob",
        "validator": true
      }
    ]
  },
  "parachains": [
    {
      "id": 2023,
      "chain": "moonriver-local",
      "chain_spec_path": "tmp/moonriver-modified-raw-spec.json",
      "collator": {
        "name": "alith",
        "command": "../target/release/moonbeam",
        "rpc_port": 33350,
        "args": [
          "--no-hardware-benchmarks",
          "--force-authoring",
          "-lparachain=debug",
          "--database=paritydb",
          "--pool-type=fork-aware"
        ]
      }
    }
  ],
  "types": {
    "Header": {
      "number": "u64",
      "parent_hash": "Hash",
      "post_state": "Hash"
    }
  }
}
