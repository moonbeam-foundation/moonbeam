{
  "$schema": "https://raw.githubusercontent.com/Moonsong-Labs/moonwall/main/packages/types/config_schema.json",
  "label": "MasterConfig ðŸŒ”",
  "defaultTestTimeout": 120000,
  "scriptsDir": "scripts/",
  "additionalRepos": [
    {
      "name": "polkadot-sdk",
      "ghAuthor": "paritytech",
      "ghRepo": "polkadot-sdk",
      "binaries": [
        {
          "name": "polkadot",
          "defaultArgs": []
        },
        {
          "name": "polkadot-execute-worker",
          "defaultArgs": []
        },
        {
          "name": "polkadot-prepare-worker",
          "defaultArgs": []
        }
      ]
    }
  ],
  "environments": [
    {
      "name": "zombie_moonbeam",
      "testFileDir": ["suites/para"],
      "include": ["**/*moonbeam*"],
      "runScripts": ["download-polkadot.sh"],
      "foundation": {
        "rtUpgradePath": "../target/release/wbuild/moonbeam-runtime/moonbeam_runtime.compact.compressed.wasm",
        "type": "zombie",
        "zombieSpec": {
          "configPath": "./configs/zombieMoonbeam.json"
        }
      }
    },
    {
      "name": "zombie_moonbase",
      "testFileDir": ["suites/para"],
      "include": ["**/*moonbase*", "**/*common*"],
      "runScripts": ["download-polkadot.sh"],
      "foundation": {
        "rtUpgradePath": "../target/release/wbuild/moonbase-runtime/moonbase_runtime.compact.compressed.wasm",
        "type": "zombie",
        "zombieSpec": {
          "configPath": "./configs/zombieAlphanet.json"
        }
      }
    },
    {
      "name": "zombie_common",
      "testFileDir": ["suites/para"],
      "include": ["**/*common*"],
      "runScripts": ["download-polkadot.sh"],
      "foundation": {
        "type": "zombie",
        "zombieSpec": {
          "configPath": "./configs/zombieMoonbeam.json"
        }
      }
    },
    {
      "name": "smoke_stagenet",
      "contracts": "contracts/",
      "testFileDir": ["/suites/smoke"],
      "runScripts": ["compile-contracts.ts compile"],
      "reporters": ["basic", "html", "json"],
      "reportFile": { "json": "./tmp/testResults.json" },
      "multiThreads": false,
      "foundation": {
        "type": "read_only",
        "launchSpec": {
          "rateLimiter": {
            "minTime": 150,
            "maxConcurrent": 5
          }
        }
      },
      "connections": [
        {
          "name": "ethers",
          "type": "ethers",
          "endpoints": ["wss://wss.api.moondev.network"]
        },
        {
          "name": "viem",
          "type": "viem",
          "endpoints": ["wss://wss.api.moondev.network"]
        },
        {
          "name": "para",
          "type": "polkadotJs",
          "endpoints": ["wss://wss.api.moondev.network"],
          "rpc": {
            "moon": {
              "isBlockFinalized": {
                "params": [
                  {
                    "name": "blockHash",
                    "type": "Hash"
                  }
                ],
                "type": "bool"
              }
            }
          }
        },
        {
          "name": "relay",
          "type": "polkadotJs",
          "endpoints": ["wss://frag-stagenet-relay-rpc-ws.g.moondev.network"]
        }
      ]
    },
    {
      "name": "smoke_moonriver",
      "testFileDir": ["/suites/smoke"],
      "contracts": "contracts/",
      "runScripts": ["compile-contracts.ts compile"],
      "reporters": ["basic", "html", "json"],
      "reportFile": { "json": "./tmp/testResults.json" },
      "multiThreads": false,
      "foundation": {
        "type": "read_only",
        "launchSpec": {
          "rateLimiter": {
            "minTime": 150,
            "maxConcurrent": 5
          }
        }
      },
      "connections": [
        {
          "name": "ethers",
          "type": "ethers",
          "endpoints": ["wss://wss.moonriver.moonbeam.network"]
        },
        {
          "name": "viem",
          "type": "viem",
          "endpoints": ["wss://wss.moonriver.moonbeam.network"]
        },
        {
          "name": "para",
          "type": "polkadotJs",
          "endpoints": ["wss://wss.moonriver.moonbeam.network"],
          "rpc": {
            "moon": {
              "isBlockFinalized": {
                "params": [
                  {
                    "name": "blockHash",
                    "type": "Hash"
                  }
                ],
                "type": "bool"
              }
            }
          }
        },
        {
          "name": "relay",
          "type": "polkadotJs",
          "endpoints": ["wss://kusama-rpc.polkadot.io"]
        }
      ]
    },
    {
      "name": "smoke_moonbeam",
      "testFileDir": ["/suites/smoke"],
      "contracts": "contracts/",
      "runScripts": ["compile-contracts.ts compile"],
      "reporters": ["basic", "html", "json"],
      "reportFile": { "json": "./tmp/testResults.json" },
      "multiThreads": false,
      "foundation": {
        "type": "read_only",
        "launchSpec": {
          "rateLimiter": {
            "minTime": 150,
            "maxConcurrent": 5
          }
        }
      },
      "connections": [
        {
          "name": "ethers",
          "type": "ethers",
          "endpoints": ["wss://wss.api.moonbeam.network"]
        },
        {
          "name": "viem",
          "type": "viem",
          "endpoints": ["wss://wss.api.moonbeam.network"]
        },
        {
          "name": "para",
          "type": "polkadotJs",
          "endpoints": ["wss://wss.api.moonbeam.network"],
          "rpc": {
            "moon": {
              "isBlockFinalized": {
                "params": [
                  {
                    "name": "blockHash",
                    "type": "Hash"
                  }
                ],
                "type": "bool"
              }
            }
          }
        },
        {
          "name": "relay",
          "type": "polkadotJs",
          "endpoints": ["wss://rpc.polkadot.io"]
        }
      ]
    },
    {
      "name": "smoke_alphanet",
      "testFileDir": ["/suites/smoke"],
      "contracts": "contracts/",
      "runScripts": ["compile-contracts.ts compile"],
      "reporters": ["basic", "html", "json"],
      "reportFile": { "json": "./tmp/testResults.json" },
      "envVars": ["DEBUG_COLORS=1"],
      "multiThreads": false,
      "foundation": {
        "type": "read_only",
        "launchSpec": {
          "rateLimiter": {
            "minTime": 150,
            "maxConcurrent": 5
          }
        }
      },
      "connections": [
        {
          "name": "ethers",
          "type": "ethers",
          "endpoints": ["wss://wss.api.moonbase.moonbeam.network"]
        },
        {
          "name": "viem",
          "type": "viem",
          "endpoints": ["wss://wss.api.moonbase.moonbeam.network"]
        },
        {
          "name": "para",
          "type": "polkadotJs",
          "endpoints": ["wss://wss.api.moonbase.moonbeam.network"],
          "rpc": {
            "moon": {
              "isBlockFinalized": {
                "description": "Just a test method",
                "params": [
                  {
                    "name": "blockHash",
                    "type": "Hash"
                  }
                ],
                "type": "bool"
              }
            }
          }
        },
        {
          "name": "relay",
          "type": "polkadotJs",
          "endpoints": [
            "wss://frag-moonbase-relay-rpc-ws.g.moonbase.moonbeam.network"
          ]
        }
      ]
    },
    {
      "name": "chopsticks_moonbeam",
      "testFileDir": ["suites/chopsticks/"],
      "foundation": {
        "type": "chopsticks",
        "launchSpec": [
          {
            "name": "mb",
            "type": "parachain",
            "configPath": "./configs/moonbeam.yml"
          }
        ]
      },
      "connections": [
        {
          "name": "MB",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:8000"]
        }
      ]
    },
    {
      "name": "dev_moonbase",
      "testFileDir": ["suites/dev/"],
      "include": ["**/*test*"],
      "contracts": "contracts/",
      "runScripts": [
        "compile-contracts.ts compile",
        "compile-wasm.ts compile -b ../target/release/moonbeam -o wasm -c moonbase-dev"
      ],
      "multiThreads": true,
      "envVars": ["DEBUG_COLORS=1", "max-old-space-size=12288"],
      "reporters": ["basic", "html", "json"],
      "reportFile": { "json": "./tmp/testResults.json" },
      "foundation": {
        "type": "dev",
        "launchSpec": [
          {
            "name": "moonbeam",
            "binPath": "../target/release/moonbeam",
            "newRpcBehaviour": true,
            "options": [
              "--ethapi=txpool",
              "--no-hardware-benchmarks",
              "--no-telemetry",
              "--wasmtime-precompiled=wasm",
              "--reserved-only",
              "--no-grandpa",
              "--no-prometheus",
              "--force-authoring",
              "--rpc-cors=all",
              "--alice",
              "--chain=moonbase-dev",
              "--sealing=manual",
              "--tmp"
            ]
          }
        ]
      }
    },
    {
      "name": "dev_moonbase_tracing",
      "testFileDir": ["suites/tracing-tests"],
      "include": ["**/*test*"],
      "contracts": "contracts/",
      "runScripts": [
        "compile-contracts.ts compile",
        "compile-wasm.ts compile -b ../target/release/moonbeam -o wasm -c moonbase-dev"
      ],
      "multiThreads": true,
      "envVars": ["DEBUG_COLORS=1"],
      "reporters": ["html", "basic"],
      "foundation": {
        "type": "dev",
        "launchSpec": [
          {
            "name": "moonbeam",
            "binPath": "../target/release/moonbeam",
            "newRpcBehaviour": true,
            "options": [
              "--ethapi=txpool,debug,trace",
              "--no-hardware-benchmarks",
              "--no-telemetry",
              "--wasmtime-precompiled=wasm",
              "--reserved-only",
              "--no-grandpa",
              "--no-prometheus",
              "--force-authoring",
              "--rpc-cors=all",
              "--alice",
              "--chain=moonbase-dev",
              "--sealing=manual",
              "--tmp",
              "--wasm-runtime-overrides=moonbase-overrides",
              "--blocks-pruning=archive"
            ]
          }
        ]
      }
    },
    {
      "name": "dev_moonbase_custom",
      "testFileDir": ["suites/dev/"],
      "contracts": "contracts/",
      "runScripts": ["compile-contracts.ts compile"],
      "foundation": {
        "type": "dev",
        "launchSpec": [
          {
            "name": "moonbeam",
            "binPath": "./tmp/moonbeam",
            "ports": { "p2pPort": 30333, "rpcPort": 9944 },
            "newRpcBehaviour": true,
            "options": [
              "--dev",
              "--no-hardware-benchmarks",
              "--no-telemetry",
              "--reserved-only",
              "--rpc-cors=all",
              "--no-grandpa",
              "--unsafe-rpc-external",
              "--sealing=manual",
              "--force-authoring",
              "--no-prometheus"
            ]
          }
        ]
      },
      "connections": [
        {
          "name": "w3",
          "type": "web3",
          "endpoints": ["ws://127.0.0.1:9944"]
        },
        {
          "name": "ethers",
          "type": "ethers",
          "endpoints": ["ws://127.0.0.1:9944"]
        },
        {
          "name": "mb",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:9944"]
        }
      ]
    },
    {
      "name": "upgrade_moonbeam",
      "testFileDir": ["suites/rt-upgrade/"],
      "foundation": {
        "type": "chopsticks",
        "rtUpgradePath": "../target/release/wbuild/moonbeam-runtime/moonbeam_runtime.compact.compressed.wasm",
        "launchSpec": [
          {
            "name": "mb",
            "type": "parachain",
            "allowUnresolvedImports": true,
            "configPath": "./configs/moonbeam.yml"
          }
        ]
      },
      "connections": [
        {
          "name": "MB",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:8000"]
        }
      ]
    },
    {
      "name": "upgrade_moonriver",
      "testFileDir": ["suites/rt-upgrade/"],
      "foundation": {
        "type": "chopsticks",
        "rtUpgradePath": "../target/release/wbuild/moonriver-runtime/moonriver_runtime.compact.compressed.wasm",
        "launchSpec": [
          {
            "name": "mb",
            "type": "parachain",
            "allowUnresolvedImports": true,
            "configPath": "./configs/moonriver.yml"
          }
        ]
      },
      "connections": [
        {
          "name": "MB",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:8000"]
        }
      ]
    },
    {
      "name": "upgrade_moonbase",
      "testFileDir": ["suites/rt-upgrade/"],
      "foundation": {
        "type": "chopsticks",
        "rtUpgradePath": "../target/release/wbuild/moonbase-runtime/moonbase_runtime.compact.compressed.wasm",
        "launchSpec": [
          {
            "name": "mb",
            "type": "parachain",
            "allowUnresolvedImports": true,
            "configPath": "./configs/alphanet.yml"
          }
        ]
      },
      "connections": [
        {
          "name": "MB",
          "type": "polkadotJs",
          "endpoints": ["ws://127.0.0.1:8000"]
        }
      ]
    }
  ]
}
