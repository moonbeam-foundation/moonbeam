{
  "name": "@moonbeam-network/test",
  "version": "1.0.0",
  "description": "Moonbeam test suite",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "bundle-types": "cd ../types-bundle && pnpm build; cd ../test",
    "typegen": "pnpm bundle-types && cd ../typescript-api && pnpm scrape && pnpm generate && pnpm build; cd ../test",
    "clean": "rm -rf node_modules",
    "fmt": "biome format .",
    "fmt:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check . --write",
    "lint": "biome lint .",
    "compile-solidity": "tsx scripts/compile-contracts.ts compile",
    "typecheck": "pnpm tsc --noEmit",
    "print-failed-tests": "tsx scripts/print-failed-tests.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
