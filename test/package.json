    "@substrate/txwrapper-substrate": "7.5.2",
    "@vitest/ui": "2.1.5",
    "@zombienet/utils": "0.0.25",
    "chalk": "5.3.0",
    "eth-object": "github:aurora-is-near/eth-object#master",
    "ethers": "*",
    "json-bigint": "1.0.0",
    "json-stable-stringify": "1.1.1",
